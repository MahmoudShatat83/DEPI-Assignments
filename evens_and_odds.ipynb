   "source": [
    "def evens_and_odds(number):\n",
    "    evens = 0\n",
    "    odds = 0\n",
    "\n",
    "    for i in range(0, number + 1):  \n",
    "        if i % 2 == 0:\n",
    "            evens += 1\n",
    "        else:\n",
    "            odds += 1\n",
    "\n",
    "    print(\"Evens:\", evens)\n",
    "    print(\"Odds:\", odds)"
   ]
